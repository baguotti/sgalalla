#!/usr/bin/expect -f

set timeout -1
set script [lindex $argv 0]
set password [lindex $argv 1]

spawn {*}$script

expect {
    "Enter new version" {
        send "\r"
        exp_continue
    }
    "*assword:" {
        send "$password\r"
        exp_continue
    }
    "*assphrase:" {
        send "$password\r"
        exp_continue
    }
    eof
}
